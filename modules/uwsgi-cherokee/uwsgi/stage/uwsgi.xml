<uwsgi>
    <pythonpath>$SERVER_PROJECT_BASEDIR/$SITE_NAME-stage</pythonpath>
    <pythonpath>$SERVER_PROJECT_BASEDIR/$SITE_NAME-stage/uwsgi/stage</pythonpath>
